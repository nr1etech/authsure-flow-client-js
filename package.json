{
  "name": "@authsure/flow-client",
  "description": "Provides client authentication functions for use with the AuthSure Flow API",
  "version": "0.0.2-alpha.7",
  "type": "module",
  "author": "NR1E, Inc",
  "publishConfig": {
    "access": "public"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/nr1etech/authsure-flow-client-js.git"
  },
  "files": [
    "**"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "prettier --check . && gts lint && cp package.json dist",
    "watch": "tsc -w",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --coverage --collectCoverageFrom='src/**/*.{ts,tsx}'",
    "lint": "gts lint",
    "clean": "rm -rf dist && rm -rf site && rm -rf coverage",
    "fix": "gts fix",
    "makepretty": "prettier --write .",
    "site": "typedoc --out site \"src/**/*.ts\" --exclude \"src/**/*.test.ts\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "gts": "^5.3.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@nr1e/commons": "^0.1.2",
    "axios": "^1.7.2",
    "jose": "^5.4.0",
    "tslib": "^2.6.3"
  },
  "peerDependencies": {
    "@nr1e/commons": "^0.1.2",
    "axios": ">=1.7.2",
    "jose": ">=5.4.0"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./index.d.ts"
    }
  }
}
